<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>SpaceInvadersSaverWin7</AssemblyName>
    <RootNamespace>SpaceInvadersSaverWin7</RootNamespace>
    <Platforms>x86;x64;AnyCPU</Platforms>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CefSharp.WinForms" Version="109.1.110" />
    <PackageReference Include="cef.redist.x86" Version="109.1.11" />
    <PackageReference Include="cef.redist.x64" Version="109.1.11" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Content\**\*.*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>
